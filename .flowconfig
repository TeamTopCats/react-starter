# # Defines a magical comment that suppresses any Flow errors on the following line. For example:
# # | // $FlowFixMe: suppressing this error until we can refactor
# # | var x : string = 123;
# suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
