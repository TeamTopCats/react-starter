{
    "presets": [
        [
            "es2015", {
                "modules": false
            }
        ],
        "react",
        "stage-2",
    ],
    "plugins": [
        "transform-runtime",
        "transform-react-jsx"
    ],
    "env": {
        "production": {
            "sourceMaps": false,
            "presets": ["react-optimize"],
            "plugins": [
                "transform-react-jsx-self",
                "transform-react-jsx-source",
                "transform-class-properties"
            ]
        },
        "development": {}
    }
}
