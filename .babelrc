{
    "presets": [
        [
            "es2015", {
                "modules": false
            }
        ],
        "stage-0",
        "react"
    ],
    "plugins": [
        "transform-react-jsx"
    ],
    "env": {
        "development": {
            "plugins": [
                "transform-react-jsx-self",
                "transform-react-jsx-source"
            ]
        },
        "production": {
            "presets": [
                "react-optimize"
            ],
            "plugins": [
                "transform-runtime",
                "transform-class-properties"
            ]
        }
    }
}
