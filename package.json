  "description": "React skeleton",
  "scripts": {
    "clean": "rm -rf ./dist",
    "start": "npm run serve | npm run dev",
    "serve": "http-server ./dist -a 0.0.0.0 -p 7777",
    "dev": "webpack-dev-server --env development --config ./webpack/webpack.config.js",
    "build": "npm run clean & webpack --env production --config ./webpack/webpack.config.js",
    "deploy": "npm run build --deploy deploy && gh-pages -d ./dist -m 'Updates --skip-ci'",
    "analyzer": "webpack --config ./webpack/webpack.config.js --json | webpack-bundle-size-analyzer"
  },
