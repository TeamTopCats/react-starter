{
  "name": "react-starter",
  "version": "v1.0.1",
  "main": "index.js",
  "description": "React skeleton",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn run clean && webpack --config ./webpack/config.js --env assemply",
    "release": "yarn run clean && webpack --config ./webpack/config.js --env release",
    "analyze": "yarn run clean && webpack --config ./webpack/config.js --env analyze",
    "publish": "yarn run lint-fix | yarn run tests | yarn run build",
    "develop": "nohup http-server ./dist &",
    "server": "webpack && babel-node server.js",
    "python-server": "python -m SimpleHTTPServer 8000",
    "start": "yarn run develop | yarn run dev",
    "stats": "webpack --config ./webpack/config.js --json --profile > stats.json",
    "pre-lint": "eslint lib/**; exit 0",
    "lint:js": "eslint ./app --ext .js[x]",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
    "lint:fix": "eslint ./app --fix .js[x]",
    "lint2": "eslint ./app -f table",
    "lint:fix3": "eslint ./app -f table --fix",
    "lint": "yarn run lint:js && yarn run lint:css",
    "dev": "webpack-dev-server --open --content-base ./dist --config ./webpack/config.js --env development",
    "production": "yarn --production",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react"
  ],
  "repository": {
    "url": "git+https://github.com/lnked/react-template.git",
