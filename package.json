{
  "name": "awesome-react-starter",
  "version": "2.2.13",
  "license": "MIT",
  "main": "index.js",
  "description": "React boilerplate, pwa, router",
  "scripts": {
    "clean": "rimraf dist",
    "svgstore": "rimraf .cache && webpack --config webpack/prebuild",
    "prepub": "npm version patch",
    "pub": "npm publish",
    "prebuild": "yarn run clean",
    "predeploy": "yarn run clean",
    "preanalyze": "yarn run clean",
    "prerelease": "yarn run clean && yarn run lint",
    "postdeploy": "gh-pages -d dist",
    "postrelease": "yarn run size",
    "serve0": "node webpack/server --env development",
    "serve": "webpack-dev-server --config webpack/dev.config --env development",
    "build": "webpack --config webpack/build.config --env assemply",
    "release": "webpack --config webpack/build.config --env release",
    "analyze": "webpack --config webpack/build.config --env analyze",
    "stats": "webpack --config webpack/build.config --json --profile > stats.json",
    "lint:scripts": "eslint ./src --ext './src/**/*.{ts,tsx,js,jsx}' --ignore-path .gitignore",
    "lint:styles": "stylelint './src/**/*.{css,less,styl,scss,sass,sss}'",
    "lint:css": "stylelint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint ./src --fix .js[x] && stylelint --fix",
    "lint": "yarn run lint:scripts && yarn run lint:css && yarn run lint:styles",
    "test": "jest",
    "start": "yarn run serve",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "size": "yarn size-limit",
    "predev-build": "./.commit development",
    "prestage-build": "./.commit staging",
    "deploy": "webpack --config webpack/build.config --env deploy",
    "eject": "",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react"
  ],
  "repository": {
    "url": "git+https://github.com/lnked/react-template.git",
    "type": "git"
  },
  "author": "Edik Bulikyan <ed.proff@gmail.com> (https://celebro.ru)",
  "size-limit": [
    {
      "path": [
        "dist/**/*.{css,js,html,svg,png}"
      ],
      "limit": "500 KB",
      "webpack": false
    }
  ],
  "config": {
    "port": "3000"
  },
