    "prebuild": "rm -rf ./dist",
    "build": "NODE_ENV=production webpack --config ./webpack/config.js",
    "serve": "nohup http-server ./dist &",
    "start": "yarn run serve | yarn run dev",
    "stats": "NODE_ENV=production webpack --config ./webpack/config.js --json --profile > stats.json",
    "lint": "eslint ./app --ext .js[x]",
    "lint-fix": "eslint ./app --fix .js[x]",
    "publish": "yarn run lint-fix | yarn run tests | yarn run build",
    "dev": "NODE_ENV=development webpack-dev-server --open --content-base ./dist --config ./webpack/config.js",
    "deploy": "yarn run build && gh-pages -d ./dist -m 'Updates --skip-ci'",
    "production": "yarn --production"
  },
  "keywords": [
    "react"
