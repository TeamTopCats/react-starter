{
  "name": "awesome-react-starter",
  "version": "2.2.17",
  "license": "MIT",
  "main": "index.js",
  "description": "React boilerplate, pwa, router",
  "scripts": {
    "clean": "rimraf dist",
    "svgstore": "rimraf .cache && webpack --config webpack/prebuild",
    "prebuild": "yarn run clean",
    "predeploy": "yarn run clean",
    "preanalyze": "yarn run clean",
    "prerelease": "yarn run clean && yarn run lint",
    "postdeploy": "gh-pages -d dist",
    "postrelease": "yarn run size",
    "serve": "webpack-dev-server --config webpack/dev.config --opts development",
    "build": "webpack --config webpack/build.config -p --opts assemply",
    "release": "webpack --config webpack/build.config -p --opts release",
    "analyze": "webpack --config webpack/build.config -p --opts analyze",
    "stats": "webpack --config webpack/build.config -p --json --profile > stats.json",
    "lint:scripts": "node_modules/.bin/eslint ./src --ext .js,.jsx",
    "lint:styles": "node_modules/.bin/stylelint 'src/**/*.{css,less,styl,scss,sass,sss}'",
    "lint:fix-scripts": "node_modules/.bin/eslint ./src --ext .js,.jsx --fix",
    "lint:fix-tslint": "node_modules/.bin/tslint 'src/**/*.{ts,tsx}' --fix",
    "lint:fix-styles": "node_modules/.bin/stylelint ./src --fix",
    "lint:fix": "yarn run lint:fix-scripts && yarn run lint:fix-tslint && yarn run lint:fix-styles",
    "lint:tslint": "node_modules/.bin/tslint 'src/**/*.{ts,tsx}'",
    "lint": "yarn run lint:scripts && yarn run lint:styles && yarn run lint:tslint",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "test": "jest",
    "start": "yarn run serve",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "size": "yarn size-limit",
    "deploy": "webpack --config webpack/build.config -p --opts deploy",
    "eject": "",
    "publish": "npm publish --access=public",
    "dev-build": "yarn release --env development",
    "prod-build": "yarn release --env production",
    "stage-build": "yarn release --env staging"
  },
  "keywords": [
    "react"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "node_modules/.bin/eslint --max-warnings=0",
      "yarn run lint:fix",
      "git add"
    ],
    "*.(js|jsx)": [
      "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty",
      "git add"
    ],
    "*.(json|css|scss|md)|.(babelrc|eslintrc)": [
      "git add"
    ]
  },
  "repository": {
    "url": "git+https://github.com/lnked/react-template.git",
    "type": "git"
  },
  "author": "Edik Bulikyan <ed.proff@gmail.com> (https://celebro.ru)",
  "size-limit": [
    {
      "path": [
        "dist/**/*.{css,js,html,svg,png}"
      ],
      "limit": "500 KB",
      "webpack": false
    }
  ],
  "config": {
    "port": "3000"
  },
