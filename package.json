    "lint-fix": "eslint ./app --fix .js[x]",
    "dev": "NODE_ENV=development webpack-dev-server --config ./webpack/config.js",
    "deploy": "npm run build && gh-pages -d ./dist -m 'Updates --skip-ci'"
  },
